{
	"#accordion": {
		"add-option": {
			"button-position": "underneath"
		}
	},
	".accordion-group": {
		"delete": true,
		"color":true,
		"animation":true
	},
	".accordion-toggle":{
		"text":true,
		"inset":true,
		"font":{
			"size": true,
			"family": false
		}
	},
	".accordion-inner": {
		"image": {
			"show-placeholder": false,
			"act-on": ".accordion-image"
		},
		"color":true,
		"resize": {
			"resize-height": true
		},
		"inset": true
	},
	".text_content": {
		"text": {
			"html": true
		},
		"resize": {
			"resize-width": true
		},
		"color": true,
		"border": true,
		"font": {
			"size": true,
			"family":false
		}
	}
}