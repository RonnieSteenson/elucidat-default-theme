{
	".tabbable": {
		"add-option": {
			"button-position": "underneath"
		}
	},
	"[data-toggle=tab]": {
		"delete": {
			"delete-parent": true
		},
		"text": true,
		"inset": true
	},
	".tab-pane": {
		"image": {
			"act-on": ".tab-inner",
			"show-placeholder": false
		},
		"color":true,
		"resize": {
			"act-on": ".tab-inner",
			"resize-height": true
		},
		"inset": true
	},
	".text_content": {
		"text": {
			"html": true
		},
		"resize": {
			"resize-width": true
		},
		"color": true,
		"border": true,
		"font": {
			"size": true,
			"family":false
		}
	}
}