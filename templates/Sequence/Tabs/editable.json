{
	".tabbable": {
		"add-option": {
			"button-position": "underneath"
		}
	},
	"[data-toggle=tab]": {
		"delete": {
			"delete-parent": true
		},
		"text": true,
		"inset": true
	},
	".tab-pane": {
		"image": {
			"show-placeholder": false
		},
		"color":true,
		"resize": {
			"resize-height": true
		},
		"inset": true
	},
	".text_content": {
		"text": {
			"html": true
		},
		"resize": true
	}
}